{
  "version": "1.2",
  "settings": [
    {
      "id": "file.name",
      "name": "File name",
      "description": "Insert a file name for your kml file, without spaces. Defaults to `moveapps_data`",
      "defaultValue": "moveapps-data",
      "type": "STRING"
    },
    {
      "id": "pts",
      "name": "Points kml",
      "description": "Select if you want to create a kml file containing the points of the data set.",
      "defaultValue": true,
      "type": "CHECKBOX"
    },
	{
      "id": "seg",
      "name": "Line segments kml",
      "description": "Select if you want to create a kml file containing the line segements of the data set.",
      "defaultValue": false,
      "type": "CHECKBOX"
    },
	{
      "id": "lins",
      "name": "Track lines kml",
      "description": "Select if you want to create a kml file containing the full lines per track of the data set.",
      "defaultValue": false,
      "type": "CHECKBOX"
    }
  ],
  "dependencies": {
    "R": [
      {
        "name": "move2"
      },
      {
        "name": "sf"
      },
      {
        "name": "dplyr"
      }
    ]
  },
